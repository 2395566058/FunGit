<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="keilen.local.mapper.ViewErmissionsMapper">
	<update id="updateOne" parameterType="String">
		update view_ermissions set ${column}=#{columnValue} where id=#{id}
	</update>
	<insert id="insertOne" parameterType="String">
		insert into view_ermissions(id) VALUES (#{id})
	</insert>
</mapper>