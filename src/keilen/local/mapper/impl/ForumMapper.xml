<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="keilen.local.mapper.ForumMapper">
	<select id="getForumName" resultType="java.util.HashMap">
		select name from forum
	</select>
	
	<select id="getIdByName" parameterType="String" resultType="String">
		select id from forum where name=#{name}
	</select>
	
	<select id="getNameById" parameterType="String" resultType="String">
		select name from forum where id=#{id}
	</select>
	
</mapper>